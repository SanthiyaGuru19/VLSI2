 module mux_4to1_tb;
    reg a,b,i0,i1,i2,i3;
    wire y;
  mux_4to1 mux_4to1_tb(
    .a(a),
    .b(b),
    .i0(i0),
    .i1(i1),
    .i2(i2),
    .i3(i3),
    .y(y));
initial begin
 $monitor("Time=%0t,a=%b,b=%b,i0=%b,i1=%b,i2=%b,i3=%b,y=%b",$time,a,b,i0,i1,i2,i3,y);
 a=0;b=0;i0=1;i1=0;i2=0;i3=0;#10;
 a=0;b=1;i0=0;i1=1;i2=0;i3=0;#10;
 a=1;b=0;i0=0;i1=0;i2=1;i3=0;#10;
 a=1;b=1;i0=0;i1=0;i2=0;i3=1;#10;
end
endmodule
